new1= int(input("whats your zipcode?"))
list.append(new)
list.append(new1)
print(list)
user = str(input("what do you wanna delete?"))
if user == "name":
    list.pop(0)
    print(list)
elif user == "age":
    list.pop(1)
    print(list)
elif user == "place":
    list.pop(2)
    print(list)
    
